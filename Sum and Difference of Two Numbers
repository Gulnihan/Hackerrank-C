#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, m, dif1, sum1;
    float a, b, dif2, sum2;

    scanf("%d %d", &n, &m);
    scanf("%f %f", &a, &b);


    sum1 = n + m;
    sum2 = a + b;
    dif1 = n - m;
    dif2 = a - b;


    printf("%d %d\n", sum1, dif1);
    printf("%.1f %.1f\n", sum2, dif2);

    return 0;
}
